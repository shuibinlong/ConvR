{
    "dataset": "FB15K237",
    "model_name": "ConvR",
    "model_hyper_params": {
      "emb_dim": 100,
      "reshape": [10, 10],
      "conv_out_channels": 100,
      "conv_kernel_size": [5, 5],
      "stride": 1,
      "input_dropout": 0.2,
      "feature_map_dropout": 0.2,
      "hidden_dropout": 0.3,
      "label_smoothing": 0.1,
      "use_bias": true
    },
    "optimizer": {
      "algorithm": "adam",
      "lr": 0.003,
      "weight_decay": 0.0
    },
    "train": {
      "batch_size": 128,
      "epochs": 5,
      "seed": 123,
      "pretrain_model": null
    },
    "eval": {
      "do_validate": true,
      "do_test": true,
      "valid_steps": 1,
      "batch_size": 128,
      "eval_func": "eval_for_tail"
    },
    "seed": 123,
    "save_model_path": "./output"
  }